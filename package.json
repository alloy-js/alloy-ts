{
  "name": "alloy-ts",
  "version": "0.1.2",
  "description": "Alloy instances in TypeScript",
  "repository": "https://github.com/alloy-js/alloy-ts.git",
  "author": {
    "name": "Tristan Dyer",
    "email": "atristandyer@gmail.com",
    "url": "https://www.github.com/atdyer"
  },
  "main": "dist/Alloy.js",
  "types": "dist/Alloy.d.ts",
  "license": "MIT",
  "keywords": [
    "alloy"
  ],
  "scripts": {
    "build-bundle": "npm run build-ts; rollup dist/Alloy.js --file alloy-$npm_package_version.js --format umd --name 'Alloy'",
    "build-docs": "typedoc src/ && touch docs/.nojekyll",
    "build-ts": "tsc -p tsconfig.json",
    "preversion": "rm -f alloy-*.*.*.js",
    "version": "npm run build-bundle; npm run build-docs; git add alloy-$npm_package_version.js docs/"
  },
  "devDependencies": {
    "rollup": "1.25.2",
    "typedoc": "0.15.0",
    "typescript": "3.6.4"
  }
}
